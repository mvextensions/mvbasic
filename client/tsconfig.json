{
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2019",
      "dom"
    ],
    "outDir": "out",
    "rootDir": "src",
    "sourceMap": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "server"
  ]
}